/*
Функция min
важность: 1решение
Задача «Hello World» для функций :)

Напишите функцию min(a,b), которая возвращает меньшее из чисел a,b.

Пример вызовов:

min(2, 5) == 2
min(3, -1) == -1
min(1, 1) == 1
*/
var min = function(a, b) {
    if (a < b) {
        return a;
    } else {
        return b;
    }
}
console.log(min(2, 5));
console.log(min(3, -1));
console.log(min(1, 1));
console.log(min(2, 5) == 2);
console.log(min(3, -1) == -1);
console.log(min(1, 1) == 1);
