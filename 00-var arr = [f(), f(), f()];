function f() {
  var value = Math.random();

  return function() {};
}

// 3 функции, каждая ссылается на свой объект переменных,
// каждый со своим значением value
var arr = [f(), f(), f()];


function f() {
  var value = Math.random();

  return value;
}

// 3 функции, каждая ссылается на свой объект переменных,
// каждый со своим значением value
var arr = [f(), f(), f()];
console.log(arr);
