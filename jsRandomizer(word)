/* Рандомайзер слов из указанного набора символов */
var randWord = function(word) {
    function compareRandom(a, b) {
      return Math.random() - 0.5;
    }
    word = word.split("").sort(compareRandom).join('');
    console.log(word); // элементы в случайном порядке, например [objeclak]
}
randWord("joeblack");
